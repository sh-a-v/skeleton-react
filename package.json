{
  "name": "skeleton-react",
  "version": "0.0.2",
  "scripts": {
    "prod": "npm install && npm run build-prod && npm run server-prod",
    "pre-prod": "npm install && npm run build-pre-prod && npm run server-pre-prod",
    "dev": "npm install && npm run build-dev && npm run server-dev",
    "build-prod": "npm run build-clean && babel-node ./webpack/webpack-builder production",
    "build-pre-prod": "npm run build-clean && babel-node ./webpack/webpack-builder pre-production",
    "build-dev": "npm run build-clean && babel-node ./webpack/webpack-builder development",
    "build-clean": "rimraf ./build/*",
    "server-prod": "node ./build/server.build.js production",
    "server-pre-prod": "node ./build/server.build.js pre-production",
    "server-dev": "parallelshell \"node ./build/server.build.js development\" \"babel-node ./webpack/webpack-server development\""
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.32",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.29",
    "colors": "^1.1.2",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "json-loader": "^0.5.2",
    "parallelshell": "^2.0.0",
    "postcss-cssnext": "^2.2.0",
    "postcss-import": "^7.1.0",
    "postcss-loader": "^0.7.0",
    "postcss-nested": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "bluebird": "^3.0.2",
    "classnames": "^2.2.0",
    "cookie": "^0.2.3",
    "counterpart": "^0.17.0",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "fs": "0.0.2",
    "history": "^1.13.0",
    "ip": "^1.0.2",
    "isomorphic-fetch": "^2.2.0",
    "json-concat": "0.0.0",
    "lodash": "^3.10.0",
    "node-libs-browser": "^0.5.2",
    "node-noop": "^1.0.0",
    "object-hash": "^0.9.2",
    "path": "^0.12.7",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-interpolate-component": "^0.8.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc4",
    "react-translate-component": "^0.10.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "url": "^0.11.0"
  }
}
