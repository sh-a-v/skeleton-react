{
  "name": "builder",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run clean && node ./webpack/webpack-builder production",
    "build-dev": "npm run clean && node ./webpack/webpack-builder development",
    "server": "npm run server-stop && forever start ./server/server.js",
