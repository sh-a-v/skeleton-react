    "css-loader": "^0.14.4",
    "cssnext": "^1.5.2",
    "express": "^4.12.4",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "forever": "^0.14.1",
    "html-webpack-plugin": "^1.4.0",
    "node-libs-browser": "^0.5.2",
    "path": "^0.11.14",
    "postcss-loader": "^0.4.3",
    "react-hot-loader": "^1.2.7",
    "rimraf": "^2.3.4",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "lodash": "^3.9.3",
    "react": "^0.13.3"
  }
